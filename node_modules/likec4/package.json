{
  "name": "likec4",
  "description": "Toolchain for your architecture diagrams",
  "version": "1.44.0",
  "license": "MIT",
  "homepage": "https://likec4.dev",
  "author": "Denis Davydkov <denis@davydkov.com>",
  "sponsor": {
    "url": "https://github.com/sponsors/davydkov"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/likec4/likec4.git",
    "directory": "packages/likec4"
  },
  "keywords": [
    "likec4",
    "architecture",
    "diagrams",
    "graph"
  ],
  "engines": {
    "node": "^20.19.0 || >=22.18.0"
  },
  "engineStrict": true,
  "bugs": "https://github.com/likec4/likec4/issues",
  "type": "module",
  "bin": "./bin/likec4.mjs",
  "exports": {
    ".": {
      "sources": "./src/index.ts",
      "default": {
        "types": "./dist/index.d.mts",
        "import": "./dist/index.mjs",
        "default": "./dist/index.mjs"
      }
    },
    "./config/schema.json": "./config/schema.json",
    "./config": {
      "sources": "./src/config/index.ts",
      "default": {
        "types": "./dist/config/index.d.mts",
        "import": "./dist/config/index.mjs",
        "default": "./dist/config/index.mjs"
      }
    },
    "./model/builder": {
      "sources": "./src/model/builder.ts",
      "default": {
        "types": "./dist/model/builder.d.mts",
        "import": "./dist/model/builder.mjs",
        "default": "./dist/model/builder.mjs"
      }
    },
    "./model": {
      "sources": "./src/model/index.ts",
      "default": {
        "types": "./dist/model/index.d.mts",
        "import": "./dist/model/index.mjs",
        "default": "./dist/model/index.mjs"
      }
    },
    "./react": {
      "sources": "./react/index.ts",
      "default": {
        "types": "./react/index.d.ts",
        "import": "./react/index.js",
        "default": "./react/index.js"
      }
    },
    "./vite-plugin-modules": {
      "types": "./vite-plugin-modules.d.ts"
    },
    "./vite-plugin/internal": {
      "sources": "./src/vite-plugin/internal.ts",
      "default": "./dist/vite-plugin/internal.mjs"
    },
    "./vite-plugin": {
      "sources": "./src/vite-plugin/index.ts",
      "default": {
        "types": "./dist/vite-plugin/index.d.mts",
        "import": "./dist/vite-plugin/index.mjs",
        "default": "./dist/vite-plugin/index.mjs"
      }
    },
    "./icons/all": {
      "types": "./icons/all.d.ts",
      "import": "./icons/all.js",
      "default": "./icons/all.js"
    },
    "./icons/*": {
      "types": "./icons/icon.d.ts",
      "import": "./icons/*",
      "default": "./icons/*"
    },
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "dependencies": {
    "@hpcc-js/wasm-graphviz": "1.15.0",
    "@vitejs/plugin-react": "^5.1.0",
    "boxen": "^8.0.1",
    "bundle-require": "^5.1.0",
    "esbuild": "0.25.11",
    "fdir": "6.4.0",
    "playwright": "1.56.1",
    "rollup": "^4.53.2",
    "std-env": "^3.9.0",
    "type-fest": "^4.41.0",
    "vite": "^7.2.2",
    "@xyflow/react": "12.9.0",
    "@xyflow/system": "0.0.71",
    "yargs": "17.7.2",
    "@likec4/core": "1.44.0"
  },
  "peerDependencies": {
    "react": "^18.x || ^19.x",
    "react-dom": "^18.x || ^19.x"
  },
  "devDependencies": {
    "@dagrejs/dagre": "1.1.5",
    "@fontsource/ibm-plex-sans": "^5.2.8",
    "@mantine/core": "8.3.7",
    "@mantine/hooks": "8.3.7",
    "@nanostores/react": "1.0.0",
    "@pandacss/dev": "^1.4.3",
    "@react-hookz/web": "^25.2.0",
    "@tabler/icons-react": "3.35.0",
    "@tanstack/react-router": "^1.114.13",
    "@tanstack/router-cli": "^1.114.13",
    "@tanstack/router-vite-plugin": "^1.114.13",
    "@types/node": "~20.19.22",
    "@types/picomatch": "^4.0.2",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.2",
    "@types/semver": "^7.7.1",
    "@types/yargs": "^17.0.33",
    "autoprefixer": "^10.4.21",
    "clsx": "^2.1.1",
    "conf": "^13.1.0",
    "defu": "^6.1.4",
    "dts-bundle-generator": "^9.5.1",
    "esbuild-node-externals": "1.18.0",
    "esm-env": "^1.2.2",
    "fast-equals": "^5.3.2",
    "get-port": "^7.1.0",
    "html-to-image": "1.11.11",
    "is-inside-container": "^1.0.0",
    "json5": "^2.2.3",
    "ky": "^1.8.2",
    "langium": "3.5.0",
    "merge-error-cause": "^5.0.2",
    "mkdirp": "^3.0.1",
    "motion": "^12.23.24",
    "nano-spawn": "^1.0.3",
    "nanostores": "1.0.1",
    "npm-run-all2": "^8.0.4",
    "p-limit": "6.2.0",
    "picomatch": "^4.0.3",
    "package-manager-detector": "1.3.0",
    "package-up": "^5.0.0",
    "postcss": "8.5.6",
    "pretty-ms": "^9.2.0",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-error-boundary": "^6.0.0",
    "react-resizable-panels": "^3.0.6",
    "react-shadow": "^20.6.0",
    "remeda": "^2.32.0",
    "semver": "^7.7.3",
    "strip-indent": "^4.1.1",
    "tinyrainbow": "^2.0.0",
    "tsx": "4.20.6",
    "turbo": "2.6.0",
    "typescript": "5.9.3",
    "ufo": "1.6.1",
    "unbuild": "3.5.0",
    "vite-plugin-dts": "^4.5.4",
    "vite-plugin-inspect": "^11.1.0",
    "vite-plugin-singlefile": "^2.2.0",
    "vitest": "4.0.8",
    "vscode-jsonrpc": "8.2.1",
    "vscode-languageserver": "9.0.1",
    "vscode-languageserver-types": "3.17.5",
    "vscode-uri": "3.1.0",
    "which": "^5.0.0",
    "wireit": "0.14.12",
    "zod": "^3.25.76",
    "@likec4/config": "1.44.0",
    "@likec4/diagram": "1.44.0",
    "@likec4/generators": "1.44.0",
    "@likec4/devops": "1.42.0",
    "@likec4/icons": "1.44.0",
    "@likec4/language-server": "1.44.0",
    "@likec4/layouts": "1.44.0",
    "@likec4/log": "1.44.0",
    "@likec4/style-preset": "1.44.0",
    "@likec4/styles": "1.44.0",
    "@likec4/tsconfig": "1.44.0"
  },
  "scripts": {
    "typecheck": "tsc --build --verbose",
    "build-cli": "unbuild",
    "build-app": "tsx scripts/bundle-app.ts",
    "build-react": "tsx scripts/bundle-react.ts",
    "build": "echo \"use pnpm turbo run build\" || true",
    "generate:app": "tsr generate && panda codegen",
    "generate:icons": "tsx --conditions=sources scripts/generate-icons.ts",
    "pack": "pnpm pack",
    "lint": "run -T eslint src/ --fix",
    "lint:package": "pnpx publint ./package.tgz",
    "clean": "pnpm rimraf --glob lib dist app/dist dev/.export dev/dist node_modules/.vite react/*.mjs react/*.d.mts icons",
    "start": "tsx --conditions=sources src/cli/index.ts",
    "dev": "pnpm cli:serve dev",
    "dev:docs:bigbank": "pnpm cli:serve ../../apps/docs/src/components/bigbank",
    "dev:docs:dynamic-view": "pnpm cli:serve ../../apps/docs/src/components/dynamic-view",
    "dev:docs:index-page": "pnpm cli:serve ../../apps/docs/src/components/index-page",
    "dev:docs:theme": "pnpm cli:serve ../../apps/docs/src/components/likec4-theme",
    "dev:playground:deployment": "pnpm cli:serve ../../apps/playground/src/examples/deployment",
    "dev:playground:dynamic": "pnpm cli:serve ../../apps/playground/src/examples/dynamic",
    "dev:template": "pnpm cli:serve ../create-likec4/template",
    "dev:e2e": "pnpm cli:serve ../../e2e/src",
    "dev:example-cloud": "pnpm cli:serve ../../examples/cloud-system",
    "dev:failed": "pnpm cli:serve ../../examples/failed",
    "dev:example-dev": "pnpm cli:serve ../../examples/diagrams-dev/likec4",
    "dev:examples": "pnpm cli:serve ../../examples",
    "cli": "tsx --conditions=sources src/cli/index.ts",
    "cli:serve": "tsx watch --conditions=sources --exclude=\"./../../{examples,e2e}/**/*.{js,ts,mjs}\" src/cli/index.ts serve",
    "cli:build": "pnpm cli build -o dev/dist dev",
    "cli:preview": "pnpm cli preview -o dev/dist dev",
    "cli:export": "pnpm cli export png -o dev/export dev",
    "cli:export:json": "pnpm cli export json -o dev/export/likec4.json dev",
    "test": "vitest run --no-isolate",
    "vitest:ui": "vitest --no-isolate --ui",
    "test:watch": "vitest"
  },
  "types": "dist/index.d.mts",
  "module": "dist/index.mjs"
}